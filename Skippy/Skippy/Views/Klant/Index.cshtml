@model List<Logic.Klant>


<h1>Klanten</h1>
<div class="container">
    <ul >
        @foreach (Logic.Klant klant in Model)
        {
        
            <a 
               asp-action="Klant"
               asp-route-id=@klant.id>


                <li >@klant.naam</li>


            </a>
        
        }
    </ul>
</div>

<div>

    @Html.ActionLink("Toevoegen", "Create")<br />

</div>
